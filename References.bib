issn = {1550-4786},
keywords = {Approximation methods,Data mining,Data models,Dimensionality Reduction,Discrete Fourier transforms,MDL,Muscles,Piecewise linear approximation,Time Series,Time series analysis,alphabet cardinality,data mining,intrinsic cardinality discovering,intrinsic representation model,minimal description length,time series,time series data mining},
month = dec,
pages = {1086--1091},
publisher = {IEEE},
shorttitle = {Data Mining (ICDM), 2011 IEEE 11th International C},
title = {{Discovering the Intrinsic Cardinality and Dimensionality of Time Series Using MDL}},
